{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://example.com/schemas/city",
	"title": "Root",
	"type": "object",
	"required": [
		"name",
		"population"
	],
	"properties": {
		"name": {
			"$id": "#root/name",
			"title": "Name",
			"type": "string",
			"default": "",
			"examples": [
				"Eggs"
			],
			"pattern": "^.*$"
		},
		"population": {
			"$id": "#root/population",
			"title": "population",
			"type": "number",
			"examples": [
				34.99
			],
			"default": 0.0
		}
	}
}