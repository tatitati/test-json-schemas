{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://example.com/country.json",
	"title": "Root",
	"type": "object",
	"required": [
		"name",
		"population"
	],
	"properties": {
		"name": {
			"$id": "#root/name",
			"title": "Name",
			"type": "string",
			"default": "",
			"examples": [
				"Eggs"
			],
			"pattern": "^.*$"
		},
		"capital": {
			"type": "object",
			"$ref": "file:city.json"
		}
	}
}